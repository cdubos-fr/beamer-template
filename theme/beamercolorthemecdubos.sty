\mode<presentation>

\DeclareOptionBeamer{color}{\def\beamer@cdubos@color{#1}}
\ExecuteOptionsBeamer{color=green}
\ProcessOptionsBeamer

\def\beamer@cdubos@colorgreen{green}%


\definecolor{cdubosdark}{rgb}{0.161, 0.651, 0.384}
\definecolor{cduboslight}{rgb}{1, 1, 1}


\setbeamercolor{palette primary}{bg=cdubosdark,fg=white}
\setbeamercolor{palette secondary}{bg=cdubosdark,fg=white}
\setbeamercolor{palette tertiary}{bg=cdubosdark,fg=white}
\setbeamercolor{palette quaternary}{bg=cdubosdark,fg=white}
\setbeamercolor{structure}{fg=cduboslight,bg=black!80!white}
\setbeamercolor{itemize item}{fg=cdubosdark,bg=black!80!white}
\setbeamercolor{section in toc}{fg=black,bg=cdubosdark}
\setbeamercolor{item projected}{bg=cdubosdark!70!black,fg=white}


\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{date}{fg=white}

\mode
<all>
